{
    "name": "optimus/heimdal",
    "autoload": {
        "psr-4": {
            "Optimus\\Heimdal\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/helpers.php"
        ]
    },
    "require": {
        "laravel/framework": "~7.0|~8.0|~9.0|~10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "1.3.*|^1.6",
        "orchestra/testbench": "4.*|^9.0",
        "phpunit/phpunit": "~8.0|^10.5",
        "php-coveralls/php-coveralls": "2.2.*|^2.7",
        "bugsnag/bugsnag-laravel": "^2.4|dev-x"
    },
    "suggest": {
        "asm89/stack-cors": "Needed to add CORS headers if add_cors_headers configuration is true",
        "sentry/sentry": "Needed to report exceptions to Sentry.io",
        "rollbar/rollbar": "Needed to report exceptions to Rollbar.com"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
